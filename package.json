{
  "name": "ecommerce-personalizado-nuevo",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "build:analyze": "ANALYZE=true astro build",
    "preview": "astro preview",
    "astro": "astro",
    "verify-env": "node scripts/verify-env.js",
