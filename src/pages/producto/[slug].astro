---
import Layout from '../../layouts/BaseLayout.astro';
import ProductDetailWithProvider from '../../components/sections/ProductDetailWithProvider';

export const prerender = false;

const { slug } = Astro.params;

if (!slug) {
  return Astro.redirect('/404');
}
---

<Layout title="Producto">
  <ProductDetailWithProvider slug={slug} client:load />
</Layout>
